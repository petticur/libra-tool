{
  "name": "libra-tool",
  "version": "0.1.0",
  "description": "TypeScript CLI tool for Libra",
  "main": "dist/index.js",
  "bin": {
    "libra-tool": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "libra",
    "cli",
    "tool"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "open-libra-sdk": "^1.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
